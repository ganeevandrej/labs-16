# include <iostream>

using namespace std;

int main()
{
     int n;
     cin >> n;
    int a[10], i;
    for ( i = 1; i < n; ++i)
        cin >> a[i];
    i = 1;
    while (i <= n) {
        cout << a[i] << endl;
        i += 2;
    }
    i=2;
    for (i = n; i; i-=2)
        cout << a[i] << endl;
    return 0;
}
