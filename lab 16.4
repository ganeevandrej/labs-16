# include <iostream>

using namespace std;

int main()

{
    const int n=10;
    int a[n];
    for (int i = 0; i < n; ++i)
    {
        cin >> a[i];
    }
    for (int i = 0; i < n / 2; ++i)
    {
        cout << a[i] << endl;
        cout << a[n - 1 - i] << endl;

    }
    if (n % 2 != 0)
        cout << a[n / 2];
    return 0;
}
