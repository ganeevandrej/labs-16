# include <iostream>

using namespace std;

int main()

{
     int N, a[5], A, B, res;
     cin >> N;
    cin >> A;
    cin >> B;
    a[0] = A;
    a[1] = B;
    res = A + B;
    a[2] = res;
    for (int i = 3; i < N; i++) {
        res += res;
        a[i] = res;
    }
    for (int i = 0; i < N; ++i) {
        cout << a[i] << endl;
    }

    return 0;
}
