# include <iostream>

using namespace std;

int main()

{
    const  int N = 5;
    double array[N];
    double A;
    cin >> A;
    array[0] = A;
    double D;
    cin >> D;

    int z = 1;    
    for (int i = 0; i < N; ++i) {
        array[i] = A * z;
        z *= D;
    }
    for (int i = 0; i < N; ++i) {
        cout << array[i] << endl;
    }

    return 0;
}
